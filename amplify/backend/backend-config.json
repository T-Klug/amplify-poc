{
  "analytics": {
    "amplifypoc": {
      "providerPlugin": "awscloudformation",
      "service": "Pinpoint"
    }
  },
  "api": {
    "amplifypoc": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "amplifypoc"
        }
      ],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AWS_IAM"
            }
          ],
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authamplifypoc"
            }
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "amplifypoc": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "ON",
        "mfaTypes": [
          "TOTP"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS",
            "REQUIRES_UPPERCASE"
          ],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL",
          "GIVEN_NAME",
          "FAMILY_NAME"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "createOpenAiChat": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ],
          "category": "api",
          "resourceName": "amplifypoc"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "notifications": {
    "amplifypoc": {
      "channels": [
        "InAppMessaging"
      ],
      "service": "Pinpoint"
    }
  },
  "parameters": {
    "AMPLIFY_analytics_Pinpoint_Id": {
      "usedBy": [
        {
          "category": "analytics",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_analytics_Pinpoint_Name": {
      "usedBy": [
        {
          "category": "analytics",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_analytics_Pinpoint_Region": {
      "usedBy": [
        {
          "category": "analytics",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_function_createOpenAiChat_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "createOpenAiChat"
        }
      ]
    },
    "AMPLIFY_function_createOpenAiChat_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "createOpenAiChat"
        }
      ]
    },
    "AMPLIFY_function_createOpenAiChat_secretsPathAmplifyAppId": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "createOpenAiChat"
        }
      ]
    },
    "AMPLIFY_notifications_Pinpoint_Id": {
      "usedBy": [
        {
          "category": "notifications",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_notifications_Pinpoint_Name": {
      "usedBy": [
        {
          "category": "notifications",
          "resourceName": "Pinpoint"
        }
      ]
    },
    "AMPLIFY_notifications_Pinpoint_Region": {
      "usedBy": [
        {
          "category": "notifications",
          "resourceName": "Pinpoint"
        }
      ]
    }
  }
}