  "parameters": {
    "AMPLIFY_function_openaiFunction_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "openaiFunction"
        }
      ]
    },
    "AMPLIFY_function_openaiFunction_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "openaiFunction"
        }
      ]
    },
    "AMPLIFY_function_openaiFunction_secretsPathAmplifyAppId": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "openaiFunction"
        }
      ]
    }
  }
